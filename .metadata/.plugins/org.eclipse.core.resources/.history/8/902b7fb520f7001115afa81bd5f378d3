package Game;

public class Move {
	
	
	private int[] moveFrom;
	private int[] moveTo;
	
	public Move(int[] moveFrom, int[] moveTo)
	{
		this.moveFrom = moveFrom;
		this.moveTo = moveTo;
	}

	public int[] getMoveFrom() {
		return moveFrom;
	}

	public void setMoveFrom(int[] moveFrom) {
		this.moveFrom = moveFrom;
	}

	public int[] getMoveTo() {
		return moveTo;
	}

	public void setMoveTo(int[] moveTo) {
		this.moveTo = moveTo;
	}

}
